<table class='rounds-table'>
  <thead>
    <tr>
      <th class="rounds-table-title"colspan="9">All Rounds</th>
    </tr>
    <tr>
      <th class="round-data-type">Holes <%= image_tag('up.png', height:50, width:50) %><%= image_tag('casa.jpg', height:50, width:50) %></th>
      <th class="round-data-type">Course <%= image_tag('casa.jpg', height:50, width:50) %><%= image_tag('casa.jpg', height:50, width:50) %></th>
      <th class="round-data-type">Date <%= image_tag('casa.jpg', height:50, width:50) %><%= image_tag('casa.jpg', height:50, width:50) %></th>
      <th class="round-data-type">Par <%= image_tag('casa.jpg', height:50, width:50) %><%= image_tag('casa.jpg', height:50, width:50) %></th>
      <th class="round-data-type">Score <%= image_tag('casa.jpg', height:50, width:50) %><%= image_tag('casa.jpg', height:50, width:50) %></th>
      <th class="round-data-type">Difficulty <%= image_tag('casa.jpg', height:50, width:50) %><%= image_tag('casa.jpg', height:50, width:50) %></th>
      <th class="round-data-type">Drinking <%= image_tag('casa.jpg', height:50, width:50) %><%= image_tag('casa.jpg', height:50, width:50) %></th>
      <th class="round-data-type">Comments <%= image_tag('casa.jpg', height:50, width:50) %><%= image_tag('casa.jpg', height:50, width:50) %></th>
      <th class="round-data-type"></th>
    </tr>
  </thead>
  <tbody class="rounds">
    <% @user.rounds.all.each do |round| %> 
      <%= render round %>
    <% end %>
  </tbody>
</table>